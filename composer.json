{
    "name": "szepeviktor/phpstan-higher-order-collections",
    "description": "Higher Order Collection Support for PHPStan",
    "type": "phpstan-extension",
    "require": {
        "php": "^7.1 || ^8.0",
        "phpstan/phpstan": "^0.12.48"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "illuminate/collections": "^8.9"
    },
    "license": "MIT",
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "SzepeViktor\\PHPStan\\HigherOrderCollection\\": "src/"
        }
    }
}
